  "name": "H2O ML Model Load",
  "type": "ml-modelload",
  "description": "Loads an H2O MOJO ML model",
  "nodeClass": "fire.nodes.h2o.NodeH2OModelLoad",
  "engine": "pyspark",
  "fields" : [
    {"name":"path", "value":"modelsavepath", "widget": "textfield", "title": "Path"}
