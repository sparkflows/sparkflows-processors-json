  "type": "ml-modelload",
  "description": "This node load the Prophet model stored in the pickel file.",
  "nodeClass": "fire.nodes.ts.NodeProphetLoad",
  "engine": "pyspark",
  "fields" : [
    {"name":"path", "value":"modelsavepath", "widget": "textfield", "title": "Path", "required": true}
