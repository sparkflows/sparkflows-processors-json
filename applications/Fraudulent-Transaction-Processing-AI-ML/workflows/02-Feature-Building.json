        {
          "name": "sql",
          "value": "WITH base AS (\r\n    SELECT\r\n        Invoice_ID,\r\n        Vendor_ID,\r\n        PO_No,\r\n        Item,\r\n        Qty_Received,\r\n        Qty_Billed,\r\n        Hours_Logged,\r\n        Hours_Billed,\r\n        Contract_Unit_Price,\r\n        Unit_Price,\r\n        CAST(Invoice_Date AS TIMESTAMP) AS Invoice_Date,\r\n        Bank_Account,\r\n        Approved_By,\r\n        Delivery_Receipt_Flag,\r\n        Invoice_Amount,\r\n\r\n        -- 1. Quantity & Hours Features\r\n        (Qty_Billed - Qty_Received) AS qty_gap,\r\n        (Qty_Billed - Qty_Received) * 1.0 / (Qty_Received + 1) AS qty_gap_ratio,\r\n        (Hours_Billed - Hours_Logged) AS hours_gap,\r\n        (Hours_Billed - Hours_Logged) * 1.0 / (Hours_Logged + 1) AS hours_gap_ratio,\r\n\r\n        -- 2. Price Features\r\n        (Unit_Price - Contract_Unit_Price) AS price_gap,\r\n        Unit_Price * 1.0 / (Contract_Unit_Price + 1) AS price_ratio,\r\n        Invoice_Amount - (Qty_Received * Contract_Unit_Price + Hours_Logged * (Contract_Unit_Price / 2.0)) AS amount_minus_expected,\r\n\r\n        -- 3. Temporal Features\r\n        CASE WHEN EXTRACT(DOW FROM CAST(Invoice_Date AS TIMESTAMP)) IN (6,0) THEN 1 ELSE 0 END AS is_weekend_invoice,\r\n        EXTRACT(DOW FROM CAST(Invoice_Date AS TIMESTAMP)) AS invoice_day,\r\n        EXTRACT(HOUR FROM CAST(Invoice_Date AS TIMESTAMP)) AS invoice_hour,\r\n\r\n        -- 5. Contract & Business Logic Features\r\n        CASE WHEN Delivery_Receipt_Flag = 0 THEN 1 ELSE 0 END AS receipt_missing,\r\n        Invoice_Amount * 1.0 / (Qty_Billed + Hours_Billed + 1) AS amount_per_unit\r\n    FROM Temp_Invoice\r\n),\r\n\r\n-- 3b. Vendor daily invoice count\r\nvendor_day AS (\r\n    SELECT\r\n        Vendor_ID,\r\n        CAST(Invoice_Date AS DATE) AS inv_date,\r\n        COUNT(*) AS vendor_day_invoice_count\r\n    FROM base\r\n    GROUP BY Vendor_ID, CAST(Invoice_Date AS DATE)\r\n),\r\n\r\n-- 4a. Duplicate combo (Vendor + PO + Amount)\r\ndup_combo AS (\r\n    SELECT\r\n        Vendor_ID, PO_No, Invoice_Amount,\r\n        COUNT(*) AS dup_combo_count\r\n    FROM base\r\n    GROUP BY Vendor_ID, PO_No, Invoice_Amount\r\n),\r\n\r\n-- 4b. Vendors per bank account\r\nbank_vendor AS (\r\n    SELECT\r\n        Bank_Account,\r\n        COUNT(DISTINCT Vendor_ID) AS bank_vendor_count\r\n    FROM base\r\n    GROUP BY Bank_Account\r\n),\r\n\r\n-- 4c. Vendors per approver\r\napprover_vendor AS (\r\n    SELECT\r\n        Approved_By,\r\n        COUNT(DISTINCT Vendor_ID) AS approver_vendor_count\r\n    FROM base\r\n    GROUP BY Approved_By\r\n),\r\n\r\n-- 5b. Frequency of items billed per vendor\r\nitem_vendor AS (\r\n    SELECT\r\n        Vendor_ID, Item,\r\n        COUNT(*) AS item_vendor_count\r\n    FROM base\r\n    GROUP BY Vendor_ID, Item\r\n)\r\n\r\n-- Final join\r\nSELECT\r\n    b.*,\r\n    vd.vendor_day_invoice_count,\r\n    dc.dup_combo_count,\r\n    bv.bank_vendor_count,\r\n    av.approver_vendor_count,\r\n    iv.item_vendor_count\r\nFROM base b\r\nLEFT JOIN vendor_day vd\r\n    ON b.Vendor_ID = vd.Vendor_ID\r\n   AND CAST(b.Invoice_Date AS DATE) = vd.inv_date\r\nLEFT JOIN dup_combo dc\r\n    ON b.Vendor_ID = dc.Vendor_ID\r\n   AND b.PO_No = dc.PO_No\r\n   AND b.Invoice_Amount = dc.Invoice_Amount\r\nLEFT JOIN bank_vendor bv\r\n    ON b.Bank_Account = bv.Bank_Account\r\nLEFT JOIN approver_vendor av\r\n    ON b.Approved_By = av.Approved_By\r\nLEFT JOIN item_vendor iv\r\n    ON b.Vendor_ID = iv.Vendor_ID\r\n   AND b.Item = iv.Item;",
          "widget": "code_editor",
          "type": "sql_mysql",
          "title": "SQL",
          "description": "SQL to be run",
