          "value": "WITH base \r\nAS (\r\n  SELECT * FROM fire_temp_table\r\n),\r\n\r\n-- 1. Invoice ID manipulation (strip all non-numeric)\r\nid_variants AS (\r\n  SELECT a.Invoice_ID, a.Vendor_ID, 'Invoice ID variant detected' AS reason\r\n  FROM base a\r\n  JOIN base b\r\n    ON a.Vendor_ID = b.Vendor_ID\r\n   AND a.Invoice_ID <> b.Invoice_ID\r\n   AND REPLACE(REPLACE(REPLACE(a.Invoice_ID, '-', ''), 'INV', ''), 'A', '') =\r\n       REPLACE(REPLACE(REPLACE(b.Invoice_ID, '-', ''), 'INV', ''), 'A', '')\r\n),\r\n\r\n-- 2. Same Vendor, Same Date, Same Amount, Different Invoice Numbers\r\nsame_vendor_amt_date AS (\r\n  SELECT b.Invoice_ID, b.Vendor_ID, 'Same vendor, same date & amount, different Invoice_ID' AS reason\r\n  FROM (\r\n    SELECT Vendor_ID, Invoice_Date, Invoice_Amount,\r\n           COUNT(DISTINCT Invoice_ID) AS invoice_count\r\n    FROM base\r\n    GROUP BY Vendor_ID, Invoice_Date, Invoice_Amount\r\n    HAVING COUNT(DISTINCT Invoice_ID) > 1\r\n  ) t\r\n  JOIN base b ON b.Vendor_ID = t.Vendor_ID \r\n              AND b.Invoice_Date = t.Invoice_Date \r\n              AND b.Invoice_Amount = t.Invoice_Amount\r\n),\r\n\r\n-- 3. Multiple Vendors, Same Bank Account\r\nsame_bank_account AS (\r\n  SELECT b.Invoice_ID, b.Vendor_ID, 'Bank account shared across vendors' AS reason\r\n  FROM base b\r\n  WHERE b.Bank_Account IN (\r\n    SELECT Bank_Account\r\n    FROM base\r\n    GROUP BY Bank_Account\r\n    HAVING COUNT(DISTINCT Vendor_ID) > 1\r\n  )\r\n),\r\n\r\n-- 4. Quantity mismatch (Invoice vs Receipt)\r\nqty_mismatch AS (\r\n  SELECT b.Invoice_ID, b.Vendor_ID, 'Quantity billed does not match receipt' AS reason\r\n  FROM base b\r\n  WHERE b.Qty IS NOT NULL AND b.Qty_Received IS NOT NULL AND b.Qty <> b.Qty_Received\r\n),\r\n\r\n-- 5. Price mismatch (Invoice vs PO contract price)\r\nprice_mismatch AS (\r\n  SELECT b.Invoice_ID, b.Vendor_ID, 'Unit price differs from contract price' AS reason\r\n  FROM base b\r\n  WHERE b.Unit_Price IS NOT NULL AND b.Contract_Unit_Price IS NOT NULL \r\n        AND b.Unit_Price <> b.Contract_Unit_Price\r\n),\r\n\r\n-- 6. Invoice Item not found in PO\r\ninvalid_item AS (\r\n  SELECT b.Invoice_ID, b.Vendor_ID, 'Item not present in PO' AS reason\r\n  FROM base b\r\n  WHERE b.Contract_Unit_Price IS NULL\r\n),\r\n\r\n-- Combine all suspicious\r\nall_flags AS (\r\n  SELECT * FROM id_variants\r\n  UNION ALL SELECT * FROM same_vendor_amt_date\r\n  UNION ALL SELECT * FROM same_bank_account\r\n  UNION ALL SELECT * FROM qty_mismatch\r\n  UNION ALL SELECT * FROM price_mismatch\r\n  UNION ALL SELECT * FROM invalid_item\r\n)\r\n\r\nSELECT b.*,\r\n       CASE WHEN f.Invoice_ID IS NOT NULL THEN 1 ELSE 0 END AS duplicate_flag,\r\n       f.reason\r\nFROM base b\r\nLEFT JOIN all_flags f\r\n  ON b.Invoice_ID = f.Invoice_ID\r\nORDER BY duplicate_flag DESC, b.Invoice_ID;",
          "widget": "code_editor",
          "type": "sql_mysql",
          "title": "SQL",
          "description": "SQL to be run",
