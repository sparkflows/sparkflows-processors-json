        {
          "name": "sql",
          "value": "SELECT t.*,\r\n       CASE \r\n           WHEN dup1.duplicate_count > 1 THEN 'Exact Duplicate - Invoice_ID + Vendor_ID + Invoice_Date + Invoice_Amount.'\r\n           WHEN dup2.duplicate_count > 1 THEN 'Same Vendor Same Amount - invoice number tampered'\r\n           WHEN dup3.duplicate_count > 1 THEN 'Duplicate Bank Payment - Bank_Account + Invoice_Amount'\r\n           ELSE 'Unique'\r\n       END AS Duplicate_Type\r\nFROM fire_temp_table t\r\nLEFT JOIN (\r\n    -- Case 1: Exact Duplicates\r\n    SELECT Invoice_ID, Vendor_ID, Invoice_Date, Invoice_Amount, COUNT(*) AS duplicate_count\r\n    FROM fire_temp_table\r\n    GROUP BY Invoice_ID, Vendor_ID, Invoice_Date, Invoice_Amount\r\n) dup1\r\n  ON t.Invoice_ID = dup1.Invoice_ID\r\n AND t.Vendor_ID = dup1.Vendor_ID\r\n AND t.Invoice_Date = dup1.Invoice_Date\r\n AND t.Invoice_Amount = dup1.Invoice_Amount\r\nLEFT JOIN (\r\n    -- Case 2: Same Vendor, Same Amount (diff invoice IDs)\r\n    SELECT Vendor_ID, Invoice_Amount, COUNT(*) AS duplicate_count\r\n    FROM fire_temp_table\r\n    GROUP BY Vendor_ID, Invoice_Amount\r\n) dup2\r\n  ON t.Vendor_ID = dup2.Vendor_ID\r\n AND t.Invoice_Amount = dup2.Invoice_Amount\r\nLEFT JOIN (\r\n    -- Case 3: Duplicate Payments by Bank Account\r\n    SELECT Bank_Account, Invoice_Amount, COUNT(*) AS duplicate_count\r\n    FROM fire_temp_table\r\n    GROUP BY Bank_Account, Invoice_Amount\r\n) dup3\r\n  ON t.Bank_Account = dup3.Bank_Account\r\n AND t.Invoice_Amount = dup3.Invoice_Amount;",
          "widget": "code_editor",
          "type": "sql_mysql",
