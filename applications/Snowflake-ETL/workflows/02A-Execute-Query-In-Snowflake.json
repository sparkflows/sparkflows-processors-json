        {
          "name": "query",
          "value": "SELECT POLICYHOLDER_NAME, CLAIM_AMOUNT, STATUS, 'Above Average Claim' AS QUERY_SOURCE\nFROM POLICY_CLAIMS_NEW_FEATURES\nWHERE CLAIM_AMOUNT > (\n    SELECT AVG(CLAIM_AMOUNT)\n    FROM POLICY_CLAIMS_NEW_FEATURES AS sub\n    WHERE sub.STATUS = POLICY_CLAIMS_NEW_FEATURES.STATUS\n)\nUNION ALL\nSELECT p.POLICYHOLDER_NAME, p.CLAIM_AMOUNT, p.STATUS, 'Max Claim for Appliance Model' AS \"Query Source\"\nFROM POLICY_CLAIMS_NEW_FEATURES p\nWHERE (p.APPLIANCE_MODEL, p.CLAIM_AMOUNT) IN (\n    SELECT APPLIANCE_MODEL, MAX(CLAIM_AMOUNT)\n    FROM POLICY_CLAIMS_NEW_FEATURES\n    WHERE POLICY_STATUS = 'Active'\n    GROUP BY APPLIANCE_MODEL\n)\nAND p.POLICY_STATUS = 'Active'\nORDER BY QUERY_SOURCE, STATUS, CLAIM_AMOUNT DESC",
          "widget": "textarea_small",
          "title": "Snowflake Query",
          "description": "",
          "required": false,
          "display": true,
          "editable": true,
          "disableRefresh": false,
          "expandable": false,
