          "name": "sql",
          "value": "SELECT\r\n  product_category,\r\n  item,\r\n  concat(\r\n    product_category, ', ', item, ' -> ',\r\n    concat_ws(' | ',\r\n      collect_list(\r\n        concat(Date, ': Actuals=', Actuals, ', Predicted=', Predicted)\r\n      )\r\n    )\r\n  ) AS description\r\nFROM ELECTRONICS_DATA\r\nGROUP BY product_category, item;",
